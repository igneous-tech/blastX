{
  "$id": "https://www.austinpowder.com/specs/schemas/geographical-location/v1.0",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Longitude and Latitude",
  "description": "A geographical coordinate on the Earth",
  "type": "object",
  "properties": {
    "latitude": {
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "longitude": {
      "type": "number",
      "minimum": -180,
      "maximum": 180
    },
    "elevation": {
      "type": "number"
    }
  },
  "required": [ "latitude", "longitude" ]
}