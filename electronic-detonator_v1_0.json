{
  "$id": "https://www.austinpowder.com/specs/schemas/electronic-detonator/v1.0",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Electronic Detonator",
  "type": "object",
  "properties": {
    "blastHoleId": {
      "type": "string"
    },
    "detNumber": {
      "type": "integer"
    },
    "branch": {
      "type": "string"
    },
    "deckNumber": {
      "type": "integer"
    },
    "delayTime": {
      "description": "Delay time in milliseconds",
      "type": "number"
    },
    "serialNumber": {
      "type": "string"
    },
    "depth": {
      "type": "number",
      "description": "Distance from the top of the hole to the detonator. Measured in m in Metric system or in ft in USCS."
    },
    "location": { "$ref": "https://www.austinpowder.com/specs/schemas/geographical-location/v1.0" }
  },
  "required": [ "blastHoleId", "detNumber" ]
}